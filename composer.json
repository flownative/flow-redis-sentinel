{
    "name": "flownative/redis-sentinel",
    "type": "library",
    "description": "Flow Redis cache backend with Sentinel support",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Lemke",
            "email": "robert@flownative.com"
        },
        {
            "name": "Neos Team",
            "email": "hello@neos.io"
        }
    ],
    "support": {
        "email": "support@flownative.com"
    },
    "require": {
        "neos/cache": "5.* || 6.* || 7.*",
        "ext-zlib": "*",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "~1.6",
        "phpunit/phpunit": "~8.1"
    },
    "autoload": {
        "psr-4": {
            "Flownative\\RedisSentinel\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flownative\\RedisSentinel\\Tests\\": "Tests"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Flownative.RedisSentinel"
        }
    }
}
