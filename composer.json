{
    "name": "flownative/redis-sentinel",
    "type": "neos-package",
    "description": "Flow Redis cache backend with Sentinel support",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Lemke",
            "email": "robert@flownative.com"
        },
        {
            "name": "Neos Team",
            "email": "hello@neos.io"
        }
    ],
    "support": {
        "email": "support@flownative.com"
    },
    "require": {
        "neos/flow": "^8.0 || ^9.0",
        "neos/cache": "^8.0 || ^9.0",
        "ext-zlib": "*",
        "predis/predis": "^2.0",
        "php": "^8.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "~1.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Flownative\\RedisSentinel\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flownative\\RedisSentinel\\Tests\\": "Tests"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Flownative.RedisSentinel"
        }
    },
    "config": {
        "allow-plugins": {
            "neos/composer-plugin": true
        }
    }
}
